        "\t\t\t\treg_y <= d;\n",
        "\n",
        "\t\t\tWHEN OTHERS => reg_y <= (OTHERS => '0');\n",
        "\n",
        "\t\tEND CASE;\n",
        "\tEND PROCESS;\n",
        "\n",
        "\ty <= reg_y;\n",
        "END comportamento;               \n",
        "               ''')\n",
