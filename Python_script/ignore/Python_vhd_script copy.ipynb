      "metadata": {},
      "outputs": [],
      "source": [
        "PORT_MAP_MAC = ('''\n",
        "\t-- MAC ja registra a saida \\n'''\n",
        "\"\tU_MAC : \"+ str(sum_name) + \" PORT MAP(\" '''\n",
        "\t\tclk, rst, \\n'''\n",
