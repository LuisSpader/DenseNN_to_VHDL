        "ARCHITECTURE comportamento OF '''+ name_mult + ''' IS\n",
        "\tSIGNAL reg_y : signed('''+ str(bits -1) + ''' DOWNTO 0);\n",
        "BEGIN\n",
        "\t--process(rst,clk,a,b,c,d,bias) \n",
        "\tPROCESS (sel, a, b, c, d)\n",
